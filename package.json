{
  "name": "nuxt-blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,css,scss,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,vue,css,scss,json,md}\"",
    "deploy": "nuxt generate && netlify deploy",
    "deploy:prod": "nuxt generate && netlify deploy --prod",
    "lint": "eslint . --ignore-pattern .gitignore",
    "lint:fix": "eslint . --fix --ignore-pattern .gitignore",
    "type-check": "vue-tsc --noEmit",
    "validate": "npm run format:check && npm run lint && npm run type-check"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@tailwindcss/typography": "^0.5.2",
    "clipboard": "^2.0.10",
    "core-js": "^3.19.3",
    "devicon": "^2.15.1",
    "ityped": "^1.0.3",
    "nuxt": "^2.15.8",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.27.0",
    "typewriter-vue": "^0.5.19",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@nuxt/image": "^0.6.2",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxtjs/svg": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.21.1",
    "nuxt-vite": "^0.3.5",
    "postcss": "^8.4.12",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.0.23",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.27"
  }
}
